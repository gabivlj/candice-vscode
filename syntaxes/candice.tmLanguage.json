										"9": {
											"name": "constant.numeric.exponent.decimal.cd",
											"patterns": [
												{
													"match": "(?<=[0-9a-fA-F])_(?=[0-9a-fA-F])",
													"name": "punctuation.separator.constant.numeric.cd"
												}
											]
										},
										"10": {
											"name": "keyword.other.unit.imaginary.cd"
										},
										"11": {
											"name": "constant.numeric.decimal.cd",
											"patterns": [
												{
													"match": "(?<=[0-9a-fA-F])_(?=[0-9a-fA-F])",
													"name": "punctuation.separator.constant.numeric.cd"
												}
											]
										},
										"12": {
											"name": "punctuation.separator.constant.numeric.cd"
